<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hybrid.orm.mapper.DeptMapper">

<select id="findOne" resultType="com.hybrid.orm.domain.Dept">
	select *
	  from dept
	 where deptno = #{deptno} 
</select>
</mapper>
